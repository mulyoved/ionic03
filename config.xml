<?xml version='1.0' encoding='utf-8'?>
<widget id="com.vixsoftware.Ionic03" version="0.8.7" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MMStars</name>
    <description>
        Blogger application
    </description>
    <author email="vixsoftwareltd@gmail.com" href="http://fin-alg.com">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />

    <!-- explain: http://learn.ionicframework.com/formulas/localstorage/ -->
    <!-- <preference name="BackupWebStorage" value="none" /> -->

	<!-- Urban Airship app credentials -->
	<preference name="com.urbanairship.production_app_key" value="PRODUCTION_APP_KEY" />
	<preference name="com.urbanairship.production_app_secret" value="PRODUCTION_APP_SECRET" />
	<preference name="com.urbanairship.development_app_key" value="dvjEEpPWSDumYs1cbqbIlw" />
	<preference name="com.urbanairship.development_app_secret" value="saLsLNGmRoC4oGItaQ5Mpw" />

	<!-- If the app is in production or not -->
	<preference name="com.urbanairship.in_production" value="false" />

	<!-- Enable push when the application launches (instead of waiting for enablePush js call).  Defaults to false -->
	<preference name="com.urbanairship.enable_push_onlaunch" value="true" />

	<!-- Only required for Android. -->
	<preference name="com.urbanairship.gcm_sender" value="44535440585" />

    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="30000" />

    <platform name="android">
        <icon src="app/res/icon/icon.png" />
        <icon src="app/res/icon/android/icon-36-ldpi.png"   density="ldpi" />
        <icon src="app/res/icon/android/icon-48-mdpi.png"   density="mdpi" />
        <icon src="app/res/icon/android/icon-72-hdpi.png"   density="hdpi" />
        <icon src="app/res/icon/android/icon-96-xhdpi.png"  density="xhdpi" />

        <splash src="app/res/screen/android/screen-ldpi-portrait.png"  density="ldpi" />
        <splash src="app/res/screen/android/screen-mdpi-portrait.png"  density="mdpi" />
        <splash src="app/res/screen/android/screen-hdpi-portrait.png"  density="hdpi" />
        <splash src="app/res/screen/android/screen-xhdpi-portrait.png" density="xhdpi" />

    </platform>

    <platform name="ios">
        <icon src="app/res/icon/ios/icon-100.png" width="100" height="100" />
        <icon src="app/res/icon/ios/icon-114.png" width="114" height="114" />
        <icon src="app/res/icon/ios/icon-120.png" width="120" height="120" />
        <icon src="app/res/icon/ios/icon-144.png" width="144" height="144" />
        <icon src="app/res/icon/ios/icon-152.png" width="152" height="152" />
        <icon src="app/res/icon/ios/icon-29.png" width="29" height="29" />
        <icon src="app/res/icon/ios/icon-40.png" width="40" height="40" />
        <icon src="app/res/icon/ios/icon-50.png" width="50" height="50" />
        <icon src="app/res/icon/ios/icon-57.png" width="57" height="57" />
        <icon src="app/res/icon/ios/icon-58.png" width="58" height="58" />
        <icon src="app/res/icon/ios/icon-72.png" width="72" height="72" />
        <icon src="app/res/icon/ios/icon-76.png" width="76" height="76" />
        <icon src="app/res/icon/ios/icon-80.png" width="80" height="80" />

        <splash src="app/res/screen/ios/screen-iphone-portrait.png"    width="320" height="480" />
        <splash src="app/res/screen/ios/screen-iphone-portrait-2x.png" width="640" height="960" />
        <splash src="app/res/screen/ios/screen-ipad-portrait.png"      width="768" height="1024" />
        <splash src="app/res/screen/ios/screen-ipad-landscape.png"     width="1024" height="768" />

    </platform>

</widget>
